@page
@model MapModel
@{
    ViewData["Title"] = "Live Map";
}

<h1 class="display-4">@ViewData["Title"]</h1>
<p>Monitor your device's health live and in real time using the ArcGIS Online web maps.</p>

<style>
    .embed-container {
        position: relative;
        padding-bottom: 73%;
        height: 0;
        max-width: 100%;
    }

        .embed-container iframe, .embed-container object, .embed-container iframe {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }

    small {
        position: absolute;
        z-index: 40;
        bottom: 0;
        margin-bottom: -15px;
    }
</style>
<div class="embed-container">
    <iframe width="1070" height="776" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" title="test" src="//matthew-jett.maps.arcgis.com/apps/Embed/index.html?webmap=60ed06a162da45e5bfe037c496c5a694&extent=-117.4604,47.6138,-117.3337,47.6617&home=true&zoom=true&previewImage=true&scale=true&search=true&searchextent=true&basemap_gallery=true&disable_scroll=false&theme=light"></iframe>
</div>